//--------------------------------------------------------------------------------------
// Constant Buffer Variables
//--------------------------------------------------------------------------------------
struct PS_INPUT
{
	float4 Pos : SV_POSITION;
};



//--------------------------------------------------------------------------------------
// Pixel Shader
//--------------------------------------------------------------------------------------
float4 PSEntry( PS_INPUT input) : SV_Target
{
    return float4(input.Pos.z,input.Pos.z,input.Pos.z,1.0f);
}
